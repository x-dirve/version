{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true,
            "dynamicImport": false,
            "exportDefaultFrom": false
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "target": "es2022",
        "keepClassNames": true
    },
    "exclude": [],
    "module": {
        "type": "commonjs"
    },
    "env": {
        "targets": {
            "node": 16
        }
    }
}